{
  "name": "@react-gis/openlayers",
  "license": "MIT",
  "version": "1.1.1",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./control": {
      "import": {
        "types": "./dist/esm/control/index.d.ts",
        "default": "./dist/esm/control/index.js"
      },
      "require": {
        "types": "./dist/cjs/control/index.d.cts",
        "default": "./dist/cjs/control/index.cjs"
      }
    },
    "./layer": {
      "import": {
        "types": "./dist/esm/layer/index.d.ts",
        "default": "./dist/esm/layer/index.js"
      },
      "require": {
        "types": "./dist/cjs/layer/index.d.cts",
        "default": "./dist/cjs/layer/index.cjs"
      }
    },
    "./map": {
      "import": {
        "types": "./dist/esm/map/index.d.ts",
        "default": "./dist/esm/map/index.js"
      },
      "require": {
        "types": "./dist/cjs/map/index.d.cts",
        "default": "./dist/cjs/map/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "vite build && publint --strict",
    "clean": "rimraf dist",
    "clean:turbo": "rimraf .turbo",
    "clean:hard": "rimraf node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "publint": "publint --strict"
  },
  "devDependencies": {
    "@react-gis/eslint": "workspace:*",
    "@react-gis/typescript": "workspace:*",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "typescript": "5.9.3",
    "vite": "7.1.12"
  },
  "peerDependencies": {
    "ol": "^10.6.0",
    "react": "^19",
    "react-dom": "^19"
  }
}
