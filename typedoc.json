{
  "$schema": "https://typedoc.org/schema.json",
  "classPropertiesFormat": "table",
  "cleanOutputDir": false,
  "enumMembersFormat": "table",
  "expandObjects": true,
  "expandParameters": true,
  "flattenOutputFiles": false,
  "formatWithPrettier": true,
  "githubPages": false,
  "groupOrder": ["Functions", "*"],
  "hideBreadcrumbs": true,
  "hideGenerator": true,
  "hidePageHeader": true,
  "hidePageTitle": true,
  "interfacePropertiesFormat": "table",
  "outputFileStrategy": "modules",
  "parametersFormat": "table",
  "propertiesFormat": "table",
  "propertyMembersFormat": "table",
  "tableColumnSettings": {
    "hideDefaults": false,
    "hideInherited": false,
    "hideModifiers": false,
    "hideOverrides": false,
    "hideSources": true,
    "hideValues": false,
    "leftAlignHeaders": true
  },
  "typeDeclarationFormat": "table",
  "useCodeBlocks": true,
  "useHTMLAnchors": false,
  "sourceLinkTemplate": "https://github.com/nickdutto/react-gis/blob/main/{path}#L{line}",
  "entryPoints": [
    "./packages/openlayers/src/control/attribution-control.tsx",
    "./packages/openlayers/src/control/full-screen-control.tsx",
    "./packages/openlayers/src/control/mouse-position-control.tsx",
    "./packages/openlayers/src/control/overview-map-control.tsx",
    "./packages/openlayers/src/control/rotate-control.tsx",
    "./packages/openlayers/src/control/scale-line-control.tsx",
    "./packages/openlayers/src/control/use-control.ts",
    "./packages/openlayers/src/control/zoom-control.tsx",
    "./packages/openlayers/src/control/zoom-slider-control.tsx",
    "./packages/openlayers/src/control/zoom-to-extent-control.tsx",
    "./packages/openlayers/src/layer/graticule-layer.tsx",
    "./packages/openlayers/src/layer/heatmap-layer.tsx",
    "./packages/openlayers/src/layer/image-layer.tsx",
    "./packages/openlayers/src/layer/tile-layer.tsx",
    "./packages/openlayers/src/layer/use-layer.ts",
    "./packages/openlayers/src/layer/vector-image-layer.tsx",
    "./packages/openlayers/src/layer/vector-layer.tsx",
    "./packages/openlayers/src/layer/vector-tile-layer.tsx",
    "./packages/openlayers/src/map/map-context.ts",
    "./packages/openlayers/src/map/map-options.ts",
    "./packages/openlayers/src/map/map-provider.tsx",
    "./packages/openlayers/src/map/map.tsx",
    "./packages/openlayers/src/map/use-map.ts"
  ],
  "entryFileName": "index",
  "readme": "none",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-frontmatter",
    "./scripts/typedoc/typedoc-frontmatter.mjs"
  ],
  "fileExtension": ".mdx",
  "out": "./apps/docs/content/docs/API-Reference/openlayers",
  "tsconfig": "./packages/openlayers/tsconfig.json"
}
