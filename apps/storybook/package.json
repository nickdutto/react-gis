{
  "name": "@react-gis/storybook",
  "private": true,
  "version": "0.1.8",
  "type": "module",
  "scripts": {
    "build": "storybook build -o ./dist",
    "clean": "rimraf dist",
    "clean:turbo": "rimraf .turbo",
    "clean:hard": "rimraf node_modules",
    "dev": "storybook dev -p 6006",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@react-gis/openlayers": "workspace:*",
    "ol": "10.7.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@react-gis/eslint": "workspace:*",
    "@react-gis/typescript": "workspace:*",
    "@storybook/addon-a11y": "10.1.4",
    "@storybook/addon-docs": "10.1.4",
    "@storybook/addon-vitest": "10.1.4",
    "@storybook/react-vite": "10.1.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/browser-playwright": "4.0.15",
    "@vitest/coverage-v8": "4.0.15",
    "@vitest/spy": "4.0.15",
    "playwright": "1.57.0",
    "storybook": "10.1.10",
    "storybook-addon-deep-controls": "0.10.0",
    "typescript": "5.9.3",
    "vite": "7.2.6",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  }
}
