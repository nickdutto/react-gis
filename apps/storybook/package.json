{
  "name": "@react-gis-openlayers/storybook",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "storybook build -o ./dist",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@react-gis-openlayers/eslint": "workspace:*",
    "@react-gis-openlayers/typescript": "workspace:*",
    "@storybook/addon-a11y": "9.1.10",
    "@storybook/addon-docs": "9.1.10",
    "@storybook/addon-vitest": "9.1.10",
    "@storybook/react-vite": "9.1.10",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "@vitejs/plugin-react": "5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/spy": "3.2.4",
    "playwright": "1.56.0",
    "storybook": "9.1.10",
    "storybook-addon-deep-controls": "0.9.5",
    "typescript": "5.9.3",
    "vite": "7.1.9",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "prettier": "@react-gis-openlayers/prettier"
}
