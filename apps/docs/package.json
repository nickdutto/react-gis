{
  "name": "@react-gis/docs",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev --turbo",
    "start": "next start",
    "cf:build": "opennextjs-cloudflare build",
    "cf:deploy": "opennextjs-cloudflare deploy",
    "cf:preview": "opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "postinstall": "fumadocs-mdx",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "format:fix": "prettier --write . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "1.10.1",
    "fumadocs-core": "15.8.5",
    "fumadocs-mdx": "12.0.3",
    "fumadocs-ui": "15.8.5",
    "lucide-react": "0.545.0",
    "next": "15.5.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@react-gis/eslint": "workspace:*",
    "@react-gis/typescript": "workspace:*",
    "@tailwindcss/postcss": "4.1.14",
    "@types/mdx": "2.0.13",
    "@types/node": "22.18.10",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.1",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.14",
    "typescript": "5.9.3",
    "wrangler": "4.42.2"
  }
}
